{
  "name": "he4rt-discord-bot-next",
  "version": "2.1.0",
  "author": "He4rt Developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:build": "tsup --sourcemap --watch",
    "dev:run": "nodemon .",
    "production": "tsup --minify --dts && node .",
    "format": "prettier --write src/**/*{.ts,.json}",
    "version:init": "generi init",
    "version:patch": "generi log patch",
    "version:minor": "generi log minor",
    "version:major": "generi log major"
  },
  "dependencies": {
    "destr": "^1.2.1",
    "discord.js": "^14.6.0",
    "uncreate": "^0.9.2"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "generi": "^0.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}