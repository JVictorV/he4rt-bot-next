{
  "name": "he4rt-discord-bot-next",
  "version": "2.2.0",
  "author": "He4rt Developers",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:build": "tsup --config tsup.development.config.ts",
    "dev:run": "nodemon .",
    "production:build": "tsup --config tsup.production.config.ts",
    "production": "pnpm run production:build && node .",
    "version:init": "generi init",
    "version:patch": "generi log patch",
    "version:minor": "generi log minor",
    "version:major": "generi log major",
    "test": "vitest run --coverage",
    "test:dev": "vitest watch",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "dependencies": {
    "cron": "^2.1.0",
    "destr": "^1.2.1",
    "discord.js": "^14.6.0",
    "uncreate": "^0.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/cron": "^2.0.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "esbuild-plugin-replace": "^1.3.0",
    "generi": "^0.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  }
}
