{
  "name": "he4rt-discord-bot-next",
  "version": "2.1.14",
  "author": "He4rt Developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:build": "tsup --config tsup.development.config.ts --env.NODE_ENV development",
    "dev:run": "nodemon .",
    "production:build": "tsup --config tsup.production.config.ts --env.NODE_ENV production",
    "production": "pnpm run production:build && node .",
    "format": "prettier --write src/**/*{.ts,.json} tests/**/*.test.ts",
    "version:init": "generi init",
    "version:patch": "generi log patch",
    "version:minor": "generi log minor",
    "version:major": "generi log major",
    "test": "vitest run --coverage",
    "test:dev": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "destr": "^1.2.1",
    "discord.js": "^14.6.0",
    "uncreate": "^0.9.2"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "esbuild-plugin-replace": "^1.3.0",
    "generi": "^0.4.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  }
}
